{
  "name": "@commitspark/graphql-api",
  "description": "GraphQL API to store and manage structured data with Git",
  "keywords": [
    "graphql",
    "graphql-api",
    "graphql-schema",
    "git",
    "gitops",
    "api"
  ],
  "version": "1.0.0-beta.6",
  "license": "ISC",
  "private": false,
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**",
    "README.md",
    "LICENSE",
    "package.json",
    "CHANGELOG.md"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.build.cjs.json",
    "build:esm": "tsc --project tsconfig.build.esm.json && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:types": "tsc --project tsconfig.build.types.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "test": "jest",
    "knip": "knip",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@apollo/server": "~5.0.0",
    "@commitspark/git-adapter": "^0.20.0",
    "@graphql-tools/schema": "^9.0.0",
    "@graphql-tools/utils": "^9.0.0"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "knip": "^5.46.5",
    "prettier": "^2.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.29.0"
  }
}
